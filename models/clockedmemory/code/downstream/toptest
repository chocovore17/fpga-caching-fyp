TD_downtoptesttest.toggle_clk ;
    %delay 10, 0;
    %load/vec4 v0x600002bc5200_0;
    %inv;
    %store/vec4 v0x600002bc5200_0, 0, 1;
    %delay 10, 0;
    %load/vec4 v0x600002bc5200_0;
    %inv;
    %store/vec4 v0x600002bc5200_0, 0, 1;
    %end;
    .scope S_0x7fbde12041e0;
T_1 ;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x600002bc46c0_0, 0, 32;
T_1.0 ;
    %load/vec4 v0x600002bc46c0_0;
    %cmpi/s 32, 0, 32;
    %jmp/0xz T_1.1, 5;
    %pushi/vec4 0, 0, 16;
    %ix/getv/s 4, v0x600002bc46c0_0;
    %store/vec4a v0x600002bc4750, 4, 0;
    %load/vec4 v0x600002bc46c0_0;
    %addi 1, 0, 32;
    %store/vec4 v0x600002bc46c0_0, 0, 32;
    %jmp T_1.0;
T_1.1 ;
    %end;
    .thread T_1;
    .scope S_0x7fbde12041e0;
T_2 ;
    %wait E_0x6000017c96b0;
    %load/vec4 v0x600002bc4630_0;
    %cmpi/e 1, 0, 1;
    %jmp/0xz  T_2.0, 4;
    %load/vec4 v0x600002bc4510_0;
    %load/vec4 v0x600002bc45a0_0;
    %pad/u 7;
    %ix/vec4 3;
    %ix/load 4, 0, 0; Constant delay
    %assign/vec4/a/d v0x600002bc4750, 0, 4;
    %pushi/vec4 1, 0, 1;
    %assign/vec4 v0x600002bc47e0_0, 0;
    %jmp T_2.1;
T_2.0 ;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x600002bc47e0_0, 0;
T_2.1 ;
    %jmp T_2;
    .thread T_2;
    .scope S_0x7fbde12041e0;
T_3 ;
    %wait E_0x6000017c96b0;
    %load/vec4 v0x600002bc42d0_0;
    %pad/u 7;
    %ix/vec4 4;
    %load/vec4a v0x600002bc4750, 4;
    %assign/vec4 v0x600002bc4480_0, 0;
    %jmp T_3;
    .thread T_3;
    .scope S_0x7fbddff060e0;
T_4 ;
    %wait E_0x6000017c96b0;
    %load/vec4 v0x600002bc4240_0;
    %dup/vec4;
    %pushi/vec4 1, 0, 2;
    %cmp/u;
    %jmp/1 T_4.0, 6;
    %dup/vec4;
    %pushi/vec4 2, 0, 2;
    %cmp/u;
    %jmp/1 T_4.1, 6;
    %pushi/vec4 1, 0, 2;
    %assign/vec4 v0x600002bc4240_0, 0;
    %jmp T_4.3;
T_4.0 ;
    %load/vec4 v0x600002bc4000_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_4.4, 8;
    %pushi/vec4 2, 0, 2;
    %assign/vec4 v0x600002bc4240_0, 0;
    %jmp T_4.5;
T_4.4 ;
    %pushi/vec4 1, 0, 2;
    %assign/vec4 v0x600002bc4240_0, 0;
T_4.5 ;
    %jmp T_4.3;
T_4.1 ;
    %load/vec4 v0x600002bc4120_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_4.6, 8;
    %pushi/vec4 1, 0, 2;
    %assign/vec4 v0x600002bc4240_0, 0;
    %jmp T_4.7;
T_4.6 ;
    %pushi/vec4 2, 0, 2;
    %assign/vec4 v0x600002bc4240_0, 0;
T_4.7 ;
    %jmp T_4.3;
T_4.3 ;
    %pop/vec4 1;
    %jmp T_4;
    .thread T_4;
    .scope S_0x7fbde1204350;
T_5 ;
    %wait E_0x6000017c96b0;
    %load/vec4 v0x600002bc4990_0;
    %load/vec4 v0x600002bc4bd0_0;
    %cmp/ne;
    %jmp/0xz  T_5.0, 4;
    %pushi/vec4 1, 0, 1;
    %assign/vec4 v0x600002bc4ab0_0, 0;
    %jmp T_5.1;
T_5.0 ;
    %load/vec4 v0x600002bc4900_0;
    %pad/u 32;
    %load/vec4 v0x600002bc4b40_0;
    %cmp/ne;
    %jmp/0xz  T_5.2, 4;
    %pushi/vec4 1, 0, 1;
    %assign/vec4 v0x600002bc4ab0_0, 0;
    %jmp T_5.3;
T_5.2 ;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x600002bc4ab0_0, 0;
T_5.3 ;
T_5.1 ;
    %load/vec4 v0x600002bc4990_0;
    %assign/vec4 v0x600002bc4bd0_0, 0;
    %load/vec4 v0x600002bc4900_0;
    %pad/u 32;
    %assign/vec4 v0x600002bc4b40_0, 0;
    %jmp T_5;
    .thread T_5;
    .scope S_0x7fbddff04760;
T_6 ;
    %vpi_call/w 3 19 "$dumpfile", "../testbench/outputs/Module/downstreamtop.vcd" {0 0 0};
    %vpi_call/w 3 20 "$dumpvars", 32'sb00000000000000000000000000000001, S_0x7fbddff04760 {0 0 0};
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x600002bc5200_0, 0, 1;
    %pushi/vec4 1, 0, 16;
    %store/vec4 v0x600002bc5050_0, 0, 16;
    %pushi/vec4 1, 0, 5;
    %store/vec4 v0x600002bc5170_0, 0, 5;
    %fork TD_downtoptesttest.toggle_clk, S_0x7fbde12044c0;
    %join;
