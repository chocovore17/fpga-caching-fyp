    %join;
    %vpi_call/w 3 45 "$display", "Write new data." {0 0 0};
    %pushi/vec4 27, 0, 5;
    %store/vec4 v0x6000019d95f0_0, 0, 5;
    %pushi/vec4 197, 0, 32;
    %store/vec4 v0x6000019d94d0_0, 0, 32;
    %fork TD_downtoptesttest.toggle_clk, S_0x7f939a205a00;
    %join;
    %fork TD_downtoptesttest.toggle_clk, S_0x7f939a205a00;
    %join;
    %vpi_call/w 3 50 "$display", "wait for state machine" {0 0 0};
    %vpi_call/w 3 51 "$display", "cancelled orders for client : [%0h]: %0h,  %0h", v0x6000019d95f0_0, v0x6000019d9560_0, v0x6000019d9710_0 {0 0 0};
    %fork TD_downtoptesttest.toggle_clk, S_0x7f939a205a00;
    %join;
    %vpi_call/w 3 55 "$display", "Write new data." {0 0 0};
    %pushi/vec4 1477, 0, 32;
    %store/vec4 v0x6000019d94d0_0, 0, 32;
    %fork TD_downtoptesttest.toggle_clk, S_0x7f939a205a00;
    %join;
    %vpi_call/w 3 59 "$display", "Read new data." {0 0 0};
    %vpi_call/w 3 60 "$display", "cancelled orders for client : [%0h]: %0h,  %0h", v0x6000019d95f0_0, v0x6000019d9560_0, v0x6000019d9710_0 {0 0 0};
    %fork TD_downtoptesttest.toggle_clk, S_0x7f939a205a00;
    %join;
    %vpi_call/w 3 64 "$display", "Read new data." {0 0 0};
    %vpi_call/w 3 65 "$display", "cancelled orders for client : [%0h]: %0h,  %0h", v0x6000019d95f0_0, v0x6000019d9560_0, v0x6000019d9710_0 {0 0 0};
    %fork TD_downtoptesttest.toggle_clk, S_0x7f939a205a00;
    %join;
    %vpi_call/w 3 69 "$display", "Read new data." {0 0 0};
    %vpi_call/w 3 70 "$display", "cancelled orders for client : [%0h]: %0h,  %0h", v0x6000019d95f0_0, v0x6000019d9560_0, v0x6000019d9710_0 {0 0 0};
    %fork TD_downtoptesttest.toggle_clk, S_0x7f939a205a00;
    %join;
    %vpi_call/w 3 74 "$display", "Read new data." {0 0 0};
    %vpi_call/w 3 75 "$display", "cancelled orders for client : [%0h]: %0h,  %0h", v0x6000019d95f0_0, v0x6000019d9560_0, v0x6000019d9710_0 {0 0 0};
